{
    "version": "0.2.0",
    "configurations": [
        {
            "cwd": "${workspaceRoot}",
            "executable": "${workspaceRoot}/build/blackpill.elf",
            "name": "Debug blackpill",
            "request": "launch",
            "type": "cortex-debug",
            "gdbPath": "/opt/arm-none-eabi/bin/arm-none-eabi-gdb",
            "servertype": "pyocd",
            // "runToEntryPoint": "main",
            "device": "stm32f411ceux",
            "serverArgs": [
                "--target",
                "stm32f411ceux",
                "--dir",
                "${workspaceRoot}",
                "--elf",
                "${workspaceRoot}/build/blackpill.elf",
                "--frequency",
                "5000000"
            ],
            "overrideRestartCommands": [
                "monitor reset halt"
            ],
            "preLaunchCommands": [
                "source ${workspaceRoot}/.vscode/rtt.gdb",
            ],
            "svdFile": "/STM32F411.svd",
            // "preLaunchTask": "prelaunch_debug",
            // "postDebugTask": "kill_nc",
            // "showDevDebugOutput": "raw",
            "overrideGDBServerStartedRegex": "GDB server listening on port",
        },
        {
            "cwd": "${workspaceRoot}",
            "executable": "${workspaceRoot}/build/blackpill",
            "name": "Attach debug wm11.elf",
            "request": "attach",
            "type": "cortex-debug",
            "gdbPath": "/opt/arm-none-eabi/bin/arm-none-eabi-gdb",
            "servertype": "pyocd",
            "device": "stm32f411ceux",
            "serverArgs": [
                "--target",
                "stm32f411ceux",
                "--dir",
                "${workspaceRoot}",
                "--elf",
                "${workspaceRoot}/build/blackpill.elf",
            ],
            "preAttachCommands": [
                "source ${workspaceRoot}/.vscode/rtt.gdb",
            ],
            "svdFile": "/STM32F411.svd",
            "overrideGDBServerStartedRegex": "GDB server listening on port",
        }
    ]
}